
<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>
    <handlers>
      <add name="dotnetBackendHandler" path="*" verb="*" modules="IsapiModule" 
           scriptProcessor="C:\inetpub\wwwroot\YourApp\bin\dotnetBackend.dll" 
           resourceType="Unspecified" requireAccess="Script" />
    </handlers>
    <aspNetCore processPath="dotnet" arguments=".\dotnetBackend.dll" stdoutLogEnabled="true" stdoutLogFile=".\logs\stdout" />
  </system.webServer>
</configuration>
